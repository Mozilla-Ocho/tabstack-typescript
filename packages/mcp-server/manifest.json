{
  "dxt_version": "0.2",
  "name": "tabstack-mcp",
  "version": "0.0.1",
  "description": "The official MCP Server for the Tabstack API",
  "author": {
    "name": "Tabstack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainless-sdks/tabstack-typescript.git"
  },
  "homepage": "https://github.com/stainless-sdks/tabstack-typescript/tree/main/packages/mcp-server#readme",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "TABSTACK_API_KEY": "${user_config.TABSTACK_API_KEY}"
      }
    }
  },
  "user_config": {
    "TABSTACK_API_KEY": {
      "title": "api_key",
      "description": "Bearer token for API authentication",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
